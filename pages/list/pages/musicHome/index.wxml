<view class="music_lyric_box view_lyric">
  <scroll-view class="music_lyric_box" scroll-y="true">
    <view class="g_pd prl20 mt20" >
      <view class="weui-search-bar__form ">
        <view class="weui-search-bar__box">
          <view class="weui-search-bar__input"></view>
        </view>
        <label class="weui-search-bar__label" bindtap="bindViewTap">
          <icon class="weui-icon-search" type="search" size="14"></icon>
          <view class="weui-search-bar__text">搜索</view>
        </label>
      </view>
    </view>
    <view class="mt20" >
      <g-swiper imgData="{{imgUrls}}"></g-swiper>
    </view>
    <g-buttonGroup bind:handleBntFn="handleBntFn" data="{{musicType}}"></g-buttonGroup>
    <g-panel>
      <view class="container" solt="g-panel-content-slot">
      </view>
    </g-panel>
  </scroll-view>
</view>